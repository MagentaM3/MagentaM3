{
  "name": "magentam3-server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc --incremental",
    "start": "npm run build && node ./dist/index.js",
    "dev": "npm run drizzle:generate --config=./src/db && npm run drizzle:migrate && nodemon src/index.ts",
    "test": "npm run drizzle:generate && npm run drizzle:migrate && cross-env NODE_ENV=development jest tests/*.test.ts --runInBand --forceExit",
    "lint": "eslint './src/**/*.ts' --fix",
    "format": "prettier --config .prettierrc.json '(src|tests)/**/*.ts' --write",
    "drizzle:push": "npx drizzle-kit push:pg --config=src/db/drizzle.config.ts",
    "drizzle:drop": "npx drizzle-kit drop --config=src/db/drizzle.config.ts",
    "drizzle:generate": "npx drizzle-kit generate --config=src/db/drizzle.config.ts",
    "drizzle:migrate": "ts-node src/db/migrate",
    "drizzle:studio": "npx drizzle-kit studio --config=src/db/drizzle.config.ts"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@spotify/web-api-ts-sdk": "^1.2.0",
    "@t3-oss/env-core": "^0.6.1",
    "@trpc/server": "^10.45.2",
    "axios": "^1.7.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.32.0",
    "drizzle-zod": "^0.5.1",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "nodemon": "^3.1.4",
    "pg": "^8.12.0",
    "postgres": "^3.4.4",
    "request": "^2.88.2",
    "superjson": "^1.13.3",
    "trpc-panel": "^1.3.4",
    "winston": "^3.13.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.7.0",
    "@tsconfig/node-lts": "^20.1.3",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.14.10",
    "@types/request": "^2.48.12",
    "drizzle-kit": "^0.23.0",
    "eslint": "^8.57.0",
    "globals": "^15.8.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.16.0"
  }
}
